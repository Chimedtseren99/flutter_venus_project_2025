void main() {
  var book1 = Book('The Great Gatsby', 'F. Scott Fitzgerald', '123456');
  print('Title: ${book1.title}, Author: ${book1.author}, ISBN: ${book1.isbn}');
  print('Available: ${book1.isAvailable}');
}

class Book {
  String title; // Номын нэр
  String author; // Зохиолч
  String isbn; // ISBN дугаар
  bool isAvailable; // Боломжит эсэх

  Book(this.title, this.author, this.isbn, {this.isAvailable = true});
}

